
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
 
generator client {
  provider = "prisma-client-js"
}
 
model User {
  id            String          @id @default(cuid())
  nickname          String?
  username         String?         @unique
  password         String?         
  salt         String?         
  image         String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}